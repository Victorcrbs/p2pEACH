<h1>Divulgar um Objeto </h1>

<%= form_for :post, url: posts_path do |f| %>
<p>
<%= f.label :Obj %><br>
<%= f.text_field :Obj %>
</p>

<p>
<%= f.label :Notes %><br>
<%= f.text_area :Notes %> 
</p>

<p>
<%= f.label :time %><br>
<%= f.text_field :time %>
</p>

<p>
<%= f.label :tags %><br>
<%= f.text_area :tags %> 
</p>

<p>
    <%= f.submit %>
</p>

<% end %>
